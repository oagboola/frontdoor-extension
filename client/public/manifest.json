{
  "short_name": "Frontdoor",
  "name": "Frontdoor Chrome Extension",
  "version": "1.0.9",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
